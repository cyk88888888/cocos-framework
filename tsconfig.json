{
  "compilerOptions": {
      "target": "es2017",
      "module": "CommonJS",
      "outDir": "./dist",
      "rootDir": "./",
      "strict": false,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "experimentalDecorators": true,
      "isolatedModules": true
  },

}